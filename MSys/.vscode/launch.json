    "version": "1.0.0",
    "configurations": [
        {
            "name": "Debug64",
            "type": "gdb",
            "request": "launch",
            "target": "<path/to/64-bit/Tcl/Tk-application/version/9.x>",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",
            "arguments": "test.tcl",
            "gdbpath": "<path/to/MSys>/mingw64/bin/gdb"
        },
        {
            "name": "Debug64-8",
            "type": "gdb",
            "request": "launch",
            "target": "<path/to/64-bit/Tcl/Tk-application/version/8.x>",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",
            "arguments": "test.tcl",
            "gdbpath": "<path/to/MSys>/mingw64/bin/gdb"
        },
        {
            "name": "Debug32",
            "type": "gdb",
            "request": "launch",
            "target": "<path/to/32-bit/Tcl/Tk-application/version/9.x>",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",
            "arguments": "test.tcl",
            "gdbpath": "<path/to/MSys>/mingw32/bin/gdb"
        },
        {
            "name": "Debug32-8",
            "type": "gdb",
            "request": "launch",
            "target": "<path/to/32-bit/Tcl/Tk-application/version/8.x>",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",
            "arguments": "test.tcl",
            "gdbpath": "<path/to/MSys>/mingw32/bin/gdb"
        }
    ]
